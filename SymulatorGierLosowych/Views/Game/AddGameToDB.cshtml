
@{
    ViewBag.Title = "AddGameToDB";
    Layout = "~/Views/Shared/_StoredLayout.cshtml";
}

@if (ViewBag.succes != "")
{
    @Html.Raw(ViewBag.succes)
    <br />
    @ViewBag.name
    @ViewBag.genreid
    @ViewBag.description
    <br />
    <br />
}
else
{
    @Html.Raw(ViewBag.error)
    @ViewBag.e
}

@ViewBag.e

